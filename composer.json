{
	"name" : "swango/stdlib",
	"license" : "Apache-2.0",
	"keywords" : [
		"php",
		"swoole"
	],
	"require" : {
		"php": ">=7.1",
		"swango/environment": "^1.0",
		"swlib/saber": "^1.0",
		"zendframework/zend-http": "^2.10"
	},
	"autoload" : {
		"psr-4" : {
			"ApiErrorException\\" : "src/ApiErrorException"
		},
		"classmap" : [
			"src/ApiErrorException.php",
			"src/BaseClient.php",
			"src/Context.php",
			"src/SysContext.php",
			"src/Json.php",
			"src/JsonDecodeFailException.php",
			"src/FinishFunc.php",
			"src/FileLog.php"
		],
		"files" : [
			"src/XString.php",
			"src/Sort.php",
			"src/Time.php"
		]
	},
	"config" : {
		"optimize-autoloader" : true,
		"sort-packages" : true
	}
}
